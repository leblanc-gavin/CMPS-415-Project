<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Subscribed Topics</title>
</head>
<body>
    <h1>Subscribed Topics</h1>
    <ul>
        <% if (userSubscription && userSubscription.subbedTopics.length > 0) { %>
            <% userSubscription.subbedTopics.forEach(function(UserSubscription) { %>
                <li><%= subbedTopics %></li>
            <% }); %>
        <% } else { %>
            <li>No subscribed topics found.</li>
        <% } %>
    </ul>
    <a href="/user-info" class="btn btn-secondary">Back to User Info</a>
</body>
</html>
